<div class="shape">

<h1 id="h1"> Forget Password</h1>
<br/>
<%= form_with url:password_reset_path do |form| %>
<div clas= "boxes">
<div class="mb-3">
            <%= form.label :email %>
            <%= form.text_field :email, class:"form-control", placeholder: "youremail@example.com" %>
        </div>
<br/>
         <div class="mb-3">
    
            <%= form.submit "Reset Password", class:"btn"%>
        
        </div>
</div>
<% end %>

</div>